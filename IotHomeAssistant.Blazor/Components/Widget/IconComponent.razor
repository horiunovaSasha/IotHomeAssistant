@using IoTHomeAssistant.Domain.Entities
<div class="icons @(_isVisible ? "d-block": "d-none")">
    @foreach (Icon item in icons)
    {
        <a class="nav-link d-inline-block" @onclick="() => OnSelectIcon.InvokeAsync(item)">
            <img src="@(item.Value)" alt="@(item.Title)" height="50" />
        </a>
    }
</div>
