<div class="light" name_attr="light">
    <h3>Light Device Configuration</h3>
    <hr />
    <div class="input-block shadow mt-1 required">
        <label for="">Title</label>
        <input type="text" class="form-control">
    </div>
    <div class="row devices">
        <div class="col-12 required">
            <select name="pluginId" class="bg-white shadow device-list w-100 deviceType2 form-control form-control-lg">
                <option value="" disabled selected>Select Plugin</option>
            </select>
        </div>
    </div>
    <div class="plugin-configurations mt-4">
        <div class="form-group">
            <label>IP Address</label>
            <input type="text" class="form-control" placeholder="Enter IP Address">
            <small id="emailHelp" class="form-text text-muted">The IP address needs to connect to your device</small>
        </div>
        <div class="form-group">
            <label>Device Key</label>
            <input type="text" class="form-control" placeholder="Enter Device Key">
            <small id="emailHelp" class="form-text text-muted">The device key can be retrived from your cloud account</small>
        </div>
    </div>
    <br />


    <div class="col-12 btn-bottom">
        <div class="float-left">
            <button class="btn btn-primary" onclick="back(this)">Back</button>
        </div>
        <div class="float-right">
            <button class="btn btn-primary save" disabled="disabled">Save</button>
        </div>
    </div>
</div>

